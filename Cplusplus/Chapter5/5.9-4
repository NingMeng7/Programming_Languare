#include <iostream>
int main()
{
	using namespace std;
	double daphne = 100;
	double cleo = 100;
	int year = 0;
	while (cleo <= daphne) {
		++year;
		daphne += 100 * 0.1;
		cleo *= (1 + 0.05);
	}
	cout << "After " << year << " year" << endl;
	cout << "Daphne: " << daphne << endl;
	cout << "Cleo: " << cleo << endl;

	return 0;
}
